[Unit]
Description=${pkg.name}
After=syslog.target

[Service]
User=${pkg.user}
ExecStart=${pkg.installFolder}/bin/${pkg.name}.jar
SuccessExitStatus=143

# Service shall be restarted in 30 seconds when the service process exits, is killed,>
# default 'no'
Restart=always 
RestartSec=30

[Install]
WantedBy=multi-user.target
